  "author": "Cuong Phan",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/core": "^4.0.1",
    "@capacitor/ios": "^4.0.1",
    "@ionic/swiftlint-config": "^1.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "swiftlint": "^1.0.1",
    "typescript": "^4.1.0"
  },
  "peerDependencies": {
    "@capacitor/core": "^4.0.1"
  },
  "files": [
    "dist/",
    "ios/",
    "CapacitorIosAutofillSavePassword.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "ios",
    "autofill",
    "keychain"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    }
  },
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/cuongpl/capacitor-ios-autofill-save-password.git"
