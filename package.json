{
  "name": "myApp",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "npm run copy:sql:wasm && ng serve",
    "build": "ng build",
    "build:web": "npm run copy:sql:wasm && ng build",
    "build:native": "npm run remove:sql:wasm && ng build",
    "watch": "npm run copy:sql:wasm && ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "dev": "ionic serve",
    "ionic:serve:before": "npm run copy:sql:wasm",
    "copy:sql:wasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm src/assets",
    "remove:sql:wasm": "rimraf src/assets/sql-wasm.wasm",
    "ionic:ios": "npm run remove:sql:wasm && ionic capacitor build ios",
    "ionic:android": "npm run remove:sql:wasm && ionic capacitor build android",
    "build-ios": "ionic cap sync ios && ionic cap build ios --configuration production",
    "build-android": "ionic cap sync android && ionic cap build android --configuration production",
    "resources_ios": "cp resources/icon_ios.png resources/icon.png && cordova-res ios --skip-config && node scripts/resources_ios.js && npx cap sync ios",
    "resources_android": "cp resources/icon_android.png resources/icon.png && cordova-res android --skip-config && node scripts/resources_android.js && npx cap sync android"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@capacitor-community/camera-preview": "^3.1.0",
    "@capacitor-community/file-opener": "^1.0.3",
    "@capacitor-community/photoviewer": "^2.0.10",
    "@capacitor-community/screen-brightness": "^4.0.0",
    "@capacitor-community/sqlite": "^4.6.1-2",
    "@capacitor/android": "4.6.1",
    "@capacitor/app": "4.1.1",
    "@capacitor/browser": "^4.0.1",
    "@capacitor/camera": "^4.1.0",
    "@capacitor/core": "4.6.1",
    "@capacitor/device": "^4.1.0",
    "@capacitor/filesystem": "^4.1.0",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.6.1",
    "@capacitor/keyboard": "4.1.0",
    "@capacitor/local-notifications": "^4.0.1",
    "@capacitor/network": "^4.1.0",
    "@capacitor/splash-screen": "^4.0.1",
    "@capacitor/status-bar": "4.1.1",
    "@capacitor/toast": "^4.1.0",
    "@capawesome/capacitor-app-update": "^2.0.1",
    "@capawesome/capacitor-badge": "^2.0.0",
    "@ionic/angular": "^6.1.9",
    "@ionic/pwa-elements": "^3.1.1",
    "@ionic/storage-angular": "^3.0.6",
    "@ngrx/store": "^15.2.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@sparkfabrik/capacitor-plugin-idfa": "^4.0.2",
    "angular-pipes": "^10.0.0",
    "angularx-qrcode": "^15.0.1",
    "capacitor-ios-autofill-save-password": "^1.1.4",
    "html2canvas": "^1.4.1",
